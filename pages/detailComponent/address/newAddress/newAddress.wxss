  box-sizing: border-box;
  font-family: "楷体";
}

.qukankan_newAddress {
  font-size: 32rpx;
}

.section {
  background-color: #fff;
  border-bottom: 1rpx solid #f5f5f5;
  padding: 24rpx 0rpx;
}

.section_picker {
  padding: 34rpx 0;
  background-color: #fff;
  border-bottom: 1rpx solid #f5f5f5;
}

.section_textarea {
  width: 100%;
  padding-top: 30rpx;
  height: 200rpx;
}

.address_textarea {
  width: 100%;
}

.btn_area {
  margin-top: 40rpx;
}

